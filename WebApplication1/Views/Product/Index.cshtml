
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <style>
        ul {
            width: 800px;
            list-style-type: none;
            float: left;
        }
        ul li{
            width:100px;
            height:50px;
            float:left;
            text-decoration-style:none;
        }
    </style>
    @functions{

//public string GetUrl(int i,int j,int m,int n)
//{
//    int i1 = 0;
//    i1 = int.TryParse(Request.QueryString["i"], out i1);
//}
        }
</head>
<body>
    <div>
       
        <ul>
            <li>样式：</li>
            @foreach (var item in ViewBag.clothType)
            {
                <li><a href="">@item.ColthTypeName</a></li>
            }
        </ul>
        <div style="clear:both"></div>
        <ul>
            <li> 颜色：</li>
           
            @foreach (var item in ViewBag.clothColor)
            {
                <li><a href="">@item.ColthColorName</a></li>
            }
        </ul>
        <div style="clear:both"></div>
        <ul>
            <li>大小：</li>
            
            @foreach (var item in ViewBag.clothSize)
            {
                <li><a href="">@item.ClothSizeName</a></li>
            }
        </ul>
        <div style="clear:both"></div>
        <ul>
            <li>价格区间：</li>
            
            @foreach (var item in ViewBag.priceRamdoms)
            {
                <li><a href="">@item.PriceRamdomName</a></li>
            }
        </ul>
        <div style="clear:both"></div>
    </div>
</body>
</html>
